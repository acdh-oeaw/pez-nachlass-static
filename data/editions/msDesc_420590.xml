   <teiHeader>
      <fileDesc>
         <titleStmt>
            <title type="main">Exzerpte BPs aus verschiedenen Werken mehrerer Autoren zu benediktinischen Schriftstellern aus den Acta Sanctorum</title>
            <title type="sub">Digital object description</title>
         </titleStmt>
         <publicationStmt>
            <publisher>Austrian Centre for Digital Humanities and Cultural Heritage</publisher>
            <idno type="unidam-verz_einh">410</idno>
