   <teiHeader>
      <fileDesc>
         <titleStmt>
            <title type="main">Abschriften zu Leben, Heiligsprechung und Translation der Gebeine des Hl. Simpert</title>
            <title type="sub">Digital object description</title>
         </titleStmt>
         <publicationStmt>
            <publisher>Austrian Centre for Digital Humanities and Cultural Heritage</publisher>
            <idno type="unidam-verz_einh">750</idno>
